[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=root
WorkingDirectory=/home/root/data/TDD-2020/sites/SITENAME/source
ExecStart=/home/root/data/TDD-2020/sites/SITENAME/virtualenv/bin/gunicorn --bind unix:/tmp/SITEIP.socket superlists.wsgi:application

[Install]
WantedBy=multi-user.target